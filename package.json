{
  "name": "com.unitrade.crm",
  "version": "780.5.1",
  "description": "Unitrade CRM",
  "author": "Ivan Lukic",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "ui5 build --config ui5-dist.yaml self-contained --dest ./build --clean-dest -a",
    "build:opt": "ui5 build --config ui5-dist.yaml --clean-dest --dest ./build --cache-mode Off --loglevel verbose",
    "start": "ui5 serve --port 8080",
    "start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
    "start:dist": "npm start -- --config ui5-dist.yaml --accept-remote-connections",
    "start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "karma-ci-cov": "karma start karma-ci-cov.conf.js",
    "test": "npm run lint && npm run karma-ci-cov"
  },
  "devDependencies": {
    "@openui5/types": "1.120.11",
    "@types/jquery": "3.5.29",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@ui5/cli": "3.9.1",
    "@ui5/ts-interface-generator": "0.8.1",
    "chart.js": "4.4.2",
    "eslint": "8.57.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-ui5": "3.0.4",
    "karma-ui5-transpile": "3.3.7",
    "rimraf": "5.0.5",
    "typescript": "5.4.2",
    "ui5-middleware-livereload": "3.0.3",
    "ui5-tooling-modules": "3.2.7",
    "ui5-tooling-transpile": "3.3.7"
  },
  "packageManager": "npm@8.1.2"
}
